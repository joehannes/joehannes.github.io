ds = [pink, blue, olive];\n\n  let [ background, setBackground ] = useState(constructBackground(backgrounds[0]));\n\n  const handleScroll = (e: React.UIEvent<HTMLElement>) => {\n    const { scrollTop } = e!.currentTarget;\n    const viewportHeight = window.innerHeight;\n\n    const screenNr = Math.floor(scrollTop / viewportHeight);\n\n    if (screenNr > 1) {\n      setBackground(constructBackground(backgrounds[2]));\n      return;\n    }\n\n    const mixRatio = 1 - (scrollTop % viewportHeight) / viewportHeight;\n    const backgroundMix = getColorMix(backgrounds[screenNr], backgrounds[screenNr +1], mixRatio);\n\n    setBackground(constructBackground(backgroundMix));\n  }\n\n  return (\n    <Router>\n      <Nav />\n      <Switch>\n        <Route path=\"/\">\n          <Background\n            style={{ background }}\n          >\n            <Scrollbars\n              onScroll={handleScroll}\n            >\n              <Dashboard/>\n            </Scrollbars>\n          </Background>\n        </Route>\n      </Switch>\n    </Router>\n  );\n}\n\nexport default App;\n",["46","47"],"/home/joehannes/Code/joehannes/joehannes-portfolio/src/reportWebVitals.ts",[],["48","49"],"/home/joehannes/Code/joehannes/joehannes-portfolio/src/components/Nav.tsx",[],"/home/joehannes/Code/joehannes/joehannes-portfolio/src/pages/Welcome.tsx",[],"/home/joehannes/Code/joehannes/joehannes-portfolio/src/routes/Dashboard.tsx",[],"/home/joehannes/Code/joehannes/joehannes-portfolio/src/pages/Motivation.tsx",[],"/home/joehannes/Code/joehannes/joehannes-portfolio/src/pages/WorkExperience.tsx",["50"],"/home/joehannes/Code/joehannes/joehannes-portfolio/src/components/Background.tsx",[],{"ruleId":"51","replacedBy":"52"},{"ruleId":"53","replacedBy":"54"},{"ruleId":"55","severity":1,"message":"56","line":1,"column":27,"nodeType":"57","messageId":"58","endLine":1,"endColumn":29},{"ruleId":"55","severity":1,"message":"59","line":1,"column":31,"nodeType":"57","messageId":"58","endLine":1,"endColumn":48},{"r